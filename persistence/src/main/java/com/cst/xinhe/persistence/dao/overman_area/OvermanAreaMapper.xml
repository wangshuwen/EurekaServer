  <select id="getOvermanAreaInfo"  resultMap="BaseResultMap">
    select
    overman_area_id, o.warning_area_id, max_overman, start_overtime, end_overtime,w.warning_area_name,w.contain_number
    from overman_area  o  left join warning_area w
    on  o.warning_area_id=w.warning_area_id

    <where>
      <if test="starOverTime != null  and endOverTime != null  ">
        AND #{starOverTime} &lt;= o.start_overtime
        AND
        #{endOverTime} &gt; o.start_overtime
      </if>
      <if test="areaName != null">
        w.warning_area_name LIKE  CONCAT('%', #{areaName}, '%')
      </if>

    </where>
  </select>




