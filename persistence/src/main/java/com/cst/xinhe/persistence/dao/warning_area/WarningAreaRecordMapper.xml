  <select id="findAreaRecordByAreaId"  resultType="java.util.HashMap">
    select  r.id, r.in_time as inTime,s.staff_name as staffName,s.group_id as groupId,s.staff_id as staffId
    from warning_area_record r  left join staff  s  on r.staff_id=s.staff_id
    <where>
      <if test="areaIdList != null and areaIdList.size()!=0">
        And r.warning_area_id  in
        <foreach  item="item" collection="areaIdList" index="index"  open="(" separator="," close=")">#{item}</foreach>
        and  r.out_time is null
      </if>
      <if test="deptIds != null and deptIds.size()!=0">
        And s.group_id in
        <foreach  item="item" collection="deptIds" index="index"  open="(" separator="," close=")">#{item}</foreach>
      </if>



      <if test="staffName != null and staffName != ''">
        AND s.staff_name LIKE CONCAT ( '%', #{staffName}, '%' )
      </if>
    </where>
  </select>

