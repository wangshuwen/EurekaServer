    <insert id="insertStaffTerminals" parameterType="java.util.List">
        INSERT INTO staff_terminal(
        terminal_id,
        create_time,
        hardware_version,
        software_version,
        co,
        co_desc,
        o2,
        o2_desc,
        ch4,
        ch4_desc,
        voice,
        voice_desc,
        wifi,
        wifi_desc,
        battery,
        battery_desc,mac_address)VALUES
        <foreach collection="list" item="it" separator=",">
            (#{it.terminalId},#{it.createTime},#{it.hardwareVersion},#{it.softwareVersion},#{it.co},#{it.coDesc},#{it.o2},#{it.o2Desc},#{it.ch4},#{it.ch4Desc},#{it.voice},#{it.voiceDesc},#{it.wifi},#{it.wifiDesc},#{it.battery},#{it.batteryDesc},#{it.macAddress})
        </foreach>
    </insert>


    <select id="selectTerminalAndPersonInfoByMac" parameterType="java.lang.String" resultType="java.util.Map" useCache="false">
        SELECT terminal_id AS terminalId,
               mac_address AS macAddress,
               staff.staff_id AS staffId,
               group_id AS groupId,
               staff_name AS staffName,
               is_person AS isPerson,
               staff_job_id AS staffJobId,
               staff_id_card AS staffIdCard
        FROM staff_terminal INNER JOIN
        staff ON staff.staff_id = staff_terminal.staff_id
        WHERE mac_address = #{mac}
    </select>
</mapper>
