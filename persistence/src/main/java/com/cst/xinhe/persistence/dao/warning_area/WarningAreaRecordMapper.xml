  </select>

  <select id="findAreaRecordByStaffId"  resultType="java.util.HashMap">
    select  r.id,a.warning_area_name as warningAreaName ,r.in_time as inTime,r.out_time as outTime,s.staff_name as staffName,s.group_id as groupId
    from warning_area_record r  left join staff  s  on r.staff_id=s.staff_id
